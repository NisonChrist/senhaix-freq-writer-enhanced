<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Width="300" Height="150"
        x:Class="SenhaixFreqWriter.Views.Common.DeviceSelectWindow"
        Title="选择设备"
        WindowStartupLocation="CenterScreen">
        <Canvas>
            <Label Canvas.Top="20" Canvas.Left="10">选择设备:</Label>
            <ComboBox Canvas.Top="50" Canvas.Left="55" Width="200" x:Name="DeviceChooseComboBox" SelectedIndex="0">
                <ComboBoxItem Content="森海克斯8800/8600"></ComboBoxItem>
                <ComboBoxItem Content="森海克斯GT12"></ComboBoxItem>
            </ComboBox>
            <Button Canvas.Top="100" Canvas.Left="130" Click="Device_OnClick">确定</Button>
        </Canvas>
</Window>
