<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Width="350" Height="300"
        x:Class="SenhaixFreqWriter.Views.Common.DebugWindow"
        Title="双击日志内容保存至文件">
    <Grid DoubleTapped="LogSaveInputElement_OnDoubleTapped">
        <ScrollViewer Name="ScViewer" >
            <SelectableTextBlock Name="tbContent" TextWrapping="Wrap"/>
        </ScrollViewer>
    </Grid>
</Window>
