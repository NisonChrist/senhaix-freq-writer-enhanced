<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Width="500" Height="200"
        CanResize="False"
        x:Class="SenhaixFreqWriter.Views.Common.AboutWindow"
        Title="关于"
        WindowStartupLocation="CenterScreen"
        xmlns:constants="clr-namespace:SenhaixFreqWriter.Properties">
    <StackPanel>
        <TextBlock>森海克斯通用写频软件（SHX8X00/GT12）</TextBlock>
        <TextBlock>适配Windows/Linux/MacOS</TextBlock>
        <TextBlock>
            版本号：<Label Content="{x:Static constants:VERSION.Version}" />
        </TextBlock>
        <TextBlock>
            提交哈希：<Label Content="{x:Static constants:VERSION.GitCommitHash}" />
        </TextBlock>
        <TextBlock>
            编译时间：<Label Content="{x:Static constants:VERSION.BuildTime}" />
        </TextBlock>
        <TextBlock>仓库：https://github.com/SydneyOwl/senhaix-freq-writer-enhanced</TextBlock>
        <TextBlock>许可证使用The Unlicense</TextBlock>
        <TextBlock>tu 73 de sydneyowl</TextBlock>
    </StackPanel>
</Window>